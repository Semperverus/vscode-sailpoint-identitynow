      },
      {
        "command": "vscode-sailpoint-identitynow.transform.evaluate.editor",
        "title": "Evaluate this transform..."
      },
      {
        "command": "vscode-sailpoint-identitynow.new-connector-rule",
        "title": "New connector rule",
        "icon": "$(add)"
      },
      {
        "command": "vscode-sailpoint-identitynow.connector-rule.upload",
        "title": "IdentityNow: Upload this code to a rule..."
      },
      {
        "command": "vscode-sailpoint-identitynow.connector-rule.validate",
        "title": "IdentityNow: Validate this code..."
      },
      {
        "command": "vscode-sailpoint-identitynow.connector-rule.export-script.editor",
        "title": "IdentityNow: Export the script from this rule..."
      },
      {
        "command": "vscode-sailpoint-identitynow.connector-rule.export-script.view",
        "title": "Export the script..."
      },
      {
        "command": "vscode-sailpoint-identitynow.identity-profile.refresh",
        "title": "Refresh all identities"
      },
      {
        "command": "vscode-sailpoint-identitynow.identity-profiles.sort.name",
        "title": "Sort by name",
        "icon": {
          "light": "resources/light/sort-alphabetically.svg",
          "dark": "resources/dark/sort-alphabetically.svg"
        }
      },
      {
        "command": "vscode-sailpoint-identitynow.identity-profiles.sort.priority",
        "title": "Sort by priority",
        "icon": {
          "light": "resources/light/sort-number.svg",
          "dark": "resources/dark/sort-number.svg"
        }
      },
      {
        "command": "vscode-sailpoint-identitynow.new-access-profile.view",
        "title": "New Access Profile",
        "icon": "$(add)"
      },
      {
        "command": "vscode-sailpoint-identitynow.new-access-profile.palette",
        "title": "IdentityNow: Add a new access profile...",
        "icon": "$(add)"
      },
      {
        "command": "vscode-sailpoint-identitynow.new-access-profile.view-icon",
        "title": "New Access Profile",
        "icon": "$(add)"
      },
      {
        "command": "vscode-sailpoint-identitynow.new-role.view",
        "title": "New Role",
        "icon": "$(add)"
      },
      {
        "command": "vscode-sailpoint-identitynow.new-role.palette",
        "title": "IdentityNow: Add a new role...",
        "icon": "$(add)"
      },
      {
        "command": "vscode-sailpoint-identitynow.new-role.view-icon",
        "title": "New Role",
        "icon": "$(add)"
      },
      {
        "command": "vscode-sailpoint-identitynow.csv.export.access-profiles.view",
        "title": "Export all Access Profiles as CSV..."
      },
      {
        "command": "vscode-sailpoint-identitynow.csv.export.access-profiles-icon.view",
        "title": "Export all Access Profiles as CSV...",
        "icon": {
          "light": "resources/light/export.svg",
          "dark": "resources/dark/export.svg"
        }
      },
      {
        "command": "vscode-sailpoint-identitynow.csv.export.roles.view",
        "title": "Export all Roles as CSV..."
      },
      {
        "command": "vscode-sailpoint-identitynow.csv.export.roles-icon.view",
        "title": "Export all Roles as CSV...",
        "icon": {
          "light": "resources/light/export.svg",
          "dark": "resources/dark/export.svg"
        }
      },
      {
        "command": "vscode-sailpoint-identitynow.csv.import.access-profiles.view",
        "title": "Import Access Profiles CSV..."
      },
      {
        "command": "vscode-sailpoint-identitynow.csv.import.access-profiles-icon.view",
        "title": "Import Access Profiles CSV...",
        "icon": {
          "light": "resources/light/import.svg",
          "dark": "resources/dark/import.svg"
        }
      },
      {
        "command": "vscode-sailpoint-identitynow.csv.import.roles.view",
        "title": "Import Roles CSV..."
      },
      {
        "command": "vscode-sailpoint-identitynow.csv.import.roles-icon.view",
        "title": "Import Roles CSV...",
        "icon": {
          "light": "resources/light/import.svg",
          "dark": "resources/dark/import.svg"
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "vscode-sailpoint-identitynow.refresh",
